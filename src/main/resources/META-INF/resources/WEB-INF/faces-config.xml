<?xml version="1.0" encoding="utf-8"?>
<faces-config version="2.0"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<name>primefaces_ultima</name>

	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>i18n.ses.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		
		<action-listener>org.primefaces.application.DialogActionListener</action-listener>
		<navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
		<view-handler>org.primefaces.application.DialogViewHandler</view-handler>
		
	</application>

	<component>
		<component-type>org.primefaces.component.UltimaMenu</component-type>
		<component-class>org.primefaces.component.UltimaMenu</component-class>
	</component>

	<render-kit>
		<renderer>
			<component-family>org.primefaces.component</component-family>
			<renderer-type>org.primefaces.component.UltimaMenuRenderer</renderer-type>
			<renderer-class>org.primefaces.component.UltimaMenuRenderer</renderer-class>
		</renderer>
	</render-kit>

	<factory>
		<exception-handler-factory>br.gov.pe.ses.starter.util.error.JsfExceptionHandlerFactory</exception-handler-factory>
	</factory>

</faces-config>